{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",	
    "branchPrefix": "PrashantR30/",
    "username": "PrashantR30",
    "gitAuthor": "Prashant <pramhit@mirantis.com>",
    "onboarding": true,
    "platform": "github",
    "forkProcessing": "enabled",
    "extends": ["config:recommended"],
    "repositories": [
      "PrashantR30/go_app"
    ],
    "golang": {
      "enabled": true
    },
    "docker": {
      "enabled": true
    },
    "packageRules": [
    {
      "matchDatasources": ["go"],
      "groupName": "golang dependencies",
      "labels": ["golang-deps"]
    },
    {
      "matchDatasources": ["docker"],
      "groupName": "docker dependencies",
      "labels": ["docker-deps"]
    }
    ],
    "dependencyDashboard": true,
    "prConcurrentLimit": 5,
    "prHourlyLimit": 2,
    "semanticCommits": "enabled"
  }
